datasource db {
  provider = "sqlserver"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model SampleEntity {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  isActive    Boolean?
  name        String?
  updatedAt   DateTime @updatedAt
}
